{
  "name": "testcontainers",
  "author": "Cristian Greco",
  "version": "1.1.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "docker",
    "testcontainers"
  ],
  "description": "TestContainers is a NodeJS library that supports tests, providing lightweight, throwaway instances of common databases, Selenium web browsers, or anything else that can run in a Docker container.",
  "license": "MIT",
  "homepage": "https://github.com/cristianrgreco/testcontainers-node#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/cristianrgreco/testcontainers-node"
  },
  "bugs": {
    "url": "https://github.com/cristianrgreco/testcontainers-node/issues"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "test": "jest",
    "lint": "tslint --fix src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dev-null": "^0.1.1",
    "dockerode": "^2.5.8",
    "get-port": "^4.1.0",
    "node-duration": "^1.0.2"
  },
  "devDependencies": {
    "@types/debug": "0.0.31",
    "@types/dockerode": "^2.5.10",
    "@types/get-port": "^4.0.1",
    "@types/jest": "^23.3.12",
    "@types/node-fetch": "^2.1.4",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format && npm run build && git add dist"
    }
  }
}
